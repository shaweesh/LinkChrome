{
  "manifest_version": 3,
  "name": "Site Links Extension",
  "version": "1.0",
  "description": "Store and manage the current site links",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  }
}